{
  "name": "verdaccio-plugin-delay-filter",
  "version": "1.1.2",
  "description": "Plugin for filtering packages in verdaccio, returning only the versions that were published N days ago. It reduces risk of being infected with 0-day or 1-day attacks with package updates which are not vetted by community.",
  "main": "lib/index.js",
  "types": "lib/src/index.d.ts",
  "files": ["lib/"],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@verdaccio/babel-preset": "^9.6.1",
    "@verdaccio/eslint-config": "^9.3.2",
    "@verdaccio/types": "^9.5.0",
    "eslint": "^6.6.0",
    "jest": "^26.0.1",
    "prettier": "^1.19.1",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.5"
  },
  "keywords": ["security,verdaccio,plugin,filter,verdaccio-plugin,node"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vsugrob/verdaccio-plugin-delay-filter.git"
  },
  "bugs": {
    "url": "https://github.com/vsugrob/verdaccio-plugin-delay-filter/issues"
  },
  "author": "Ansile <ansilet@yandex.ru>, vsugrob <vsugrob@hotmail.com>",
  "license": "MIT",
  "scripts": {
    "prepack": "npm run build",
    "release": "standard-version -a",
    "build": "npm run build:types && npm run build:js",
    "build:js": "babel src/ --out-dir lib --extensions \".ts,.tsx\" --ignore \"**/*.test.ts\"",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest",
    "test:debug": "jest --no-coverage --runInBand --verbose",
    "lint": "eslint \"**/*.{js,ts}\""
  }
}
